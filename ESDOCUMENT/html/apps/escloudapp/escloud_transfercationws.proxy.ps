/**
 * 文件移交
 * @author xuekun
 *
 */
class ProxyEscloudappEscloud_transfercationws extends AgentProxyAbstract {
	const SERVICE_NAME = "escloud_transfercationws";
	/**
	 * 获得历史移交单
	 * 
	 * @author xuekun 2015年1月12日 下午2:26:02
	 * @param unknown $params        	
	 * @return return_type
	 */
	public function createDestroyForm($params) {
		return $this->post ( self::SERVICE_NAME, 'createTransferForm', $params, "application/json;charset=UTF-8" );
	}
	/**
	 * 获得历史移交单
	 * 
	 * @author xuekun 2015年1月12日 下午2:25:45
	 * @param unknown $param        	
	 * @return return_type
	 */
	public function getHistoryTransfercationList($param) {
		return $this->post ( self::SERVICE_NAME, 'getHistoryTransfercationList', $param, "application/json;charset=UTF-8" );
	}
	/**
	 * 根据数据path获取可移交的path集合
	 * @author xuekun 2015年1月12日 下午5:26:33
	 * @param unknown $param
	 * @return return_type
	 */
	public function getTransferDataList($param)
	{
		return $this->post(self::SERVICE_NAME, 'getTransferDataList', $param, "application/json;charset=UTF-8");
	}
	/**
	 * 获取可销毁数据的path
	 * @author xuekun
	 */
	public function getTransferPathList($param)
	{
		return $this->post(self::SERVICE_NAME, 'getTransferPathList', $param, "application/json;charset=UTF-8");
	}
}